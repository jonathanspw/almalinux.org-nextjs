    width: 100%;
	
}

.scroll-box .elementor-column-wrap:after {
    content: '';
    position: absolute;
    top: 2.5%;
    left: 50%;
    transform: translateX(-50%);
    width: 50%;
    height: 2.5%;
    background-color: rgb(58, 58, 58);
    border-radius: 0 0 10px 10px;
}

